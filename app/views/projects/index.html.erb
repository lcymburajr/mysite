<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>

 <% if logged_in? %>
  <%= link_to "Log out", logout_path, method: :delete %>
</br>
  <%= link_to 'add new', new_project_path %>
<% else %>
  # Links for non-logged-in-users
<% end %>


<% if flash[:notice] %>
  <p><%= flash[:notice] %></p>
<% end %>

<ul>
  <% @project.each do |p| %>
    <li><%= p.title %></li>
    <li><%= link_to(image_tag(p.image.url(:large)), edit_project_path(p.id)) %></li>
  <% end %>
</ul>

<%= link_to 'add new', new_project_path %>